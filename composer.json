{
    "name": "directorytree/metrics",
    "description": "Record metrics in your Laravel application",
    "type": "library",
    "require": {
        "illuminate/support": "^12.31",
        "illuminate/database": "^12.31"
    },
    "require-dev": {
        "orchestra/testbench": "^10.6",
        "laravel/pint": "^1.25",
        "pestphp/pest": "^3.8"
    },
    "license": "MIT",
    "archive": {
        "exclude": [
            "/tests"
        ]
    },
    "autoload": {
        "psr-4": {
            "DirectoryTree\\Metrics\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DirectoryTree\\Metrics\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Steve Bauman",
            "email": "steven_bauman@outlook.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
