{
    "name": "directorytree/metrics",
    "description": "Record metrics in your Laravel application",
    "type": "library",
    "require": {
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/collections": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "pestphp/pest": "^1.0|^2.0|^3.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0"
    },
    "license": "MIT",
    "archive": {
        "exclude": [
            "/tests"
        ]
    },
    "autoload": {
        "psr-4": {
            "DirectoryTree\\Metrics\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "DirectoryTree\\Metrics\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Steve Bauman",
            "email": "steven_bauman@outlook.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "DirectoryTree\\Metrics\\MetricServiceProvider"
            ],
            "aliases": {
                "Metrics": "DirectoryTree\\Metrics\\Facades\\Metrics"
            }
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
